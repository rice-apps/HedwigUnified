{
    "name": "hedwig-backend",
    "version": "1.0.0",
    "description": "GraphQL backend for Hedwig",
    "repository": {
        "type": "git"
    },
    "main": "src/index.js",
    "scripts": {
        "clean": "rm -rf build",
        "compile": "babel src -d build",
        "format": "prettier --write .",
        "lint": "eslint --fix .",
        "pkg": "pkg build/index.js -o build/HedwigBackend --options trace-warnings",
        "refresh": "rm -rf build node_modules && rm package-lock.json",
        "start": "nodemon",
        "server": "node --trace-warnings build/index.js"
    },
    "author": "vms2@rice.edu",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.10.4",
        "@babel/core": "^7.11.4",
        "@babel/node": "^7.8.7",
        "@babel/plugin-transform-runtime": "^7.10.4",
        "@babel/preset-env": "^7.9.0",
        "eslint": "^7.7.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "nodemon": "^2.0.4",
        "pkg": "^4.4.9",
        "prettier": "^2.0.5"
    },
    "dependencies": {
        "@babel/runtime": "^7.11.2",
        "apollo-server-express": "^2.16.1",
        "axios": "^0.19.2",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "graphql": "^14.0.0",
        "graphql-compose": "^7.19.4",
        "graphql-compose-mongoose": "^7.5.0",
        "jsonwebtoken": "^8.5.1",
        "loglevel": "^1.6.8",
        "mongoose": "^5.10.0",
        "square-connect": "^4.20200812.2",
        "uuid": "^8.3.0",
        "xml2js": "^0.4.23"
    }
}
