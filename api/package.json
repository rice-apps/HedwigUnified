{
  "name": "hedwig-backend",
  "version": "0.1.0",
  "description": "GraphQL backend for Hedwig",
  "repository": {
    "type": "git"
  },
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "babel src -d build",
    "lint": "prettier-standard --lint",
    "pkg": "pkg build/index.js -o build/HedwigBackend --options trace-warnings",
    "refresh": "rm -rf build node_modules && rm package-lock.json",
    "start": "node --trace-warnings build/index.js",
    "dev": "nodemon",
    "create-env": "printenv > .env"
  },
  "author": "vms2@rice.edu",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/qs": "6.9.4",
    "@types/shopify-buy": "^2.10.3",
    "nodemon": "^2.0.6",
    "pkg": "^4.4.9",
    "prettier-standard": "^16.4.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "apollo-server-express": "^2.19.1",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.3.0",
    "graphql-compose": "^7.23.0",
    "graphql-compose-mongoose": "^9.0.0",
    "graphql-redis-subscriptions": "^2.3.1",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.7.1",
    "mongoose": "^5.11.8",
    "node-fetch": "^2.6.1",
    "shopify-api-node": "^3.6.0",
    "shopify-buy": "^2.11.0",
    "square": "^8.0.1",
    "twilio": "^3.54.1",
    "uuid": "^8.3.2",
    "xml2js": "^0.4.23"
  }
