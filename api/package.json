{
  "name": "hedwig-backend",
  "version": "0.1.0",
  "description": "GraphQL backend for Hedwig",
  "repository": {
    "type": "git"
  },
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "build": "babel src -d build",
    "lint": "prettier-standard --lint",
    "pkg": "pkg build/index.js -o build/HedwigBackend --options trace-warnings",
    "refresh": "rm -rf build node_modules && rm package-lock.json",
    "start": "node --trace-warnings build/index.js",
    "dev": "nodemon",
    "create-env": "printenv > .env"
  },
  "author": "vms2@rice.edu",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@types/shopify-buy": "^2.10.3",
    "nodemon": "^2.0.6",
    "pkg": "^4.4.9",
    "prettier-standard": "^16.4.1",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "apollo-server-express": "^2.19.0",
    "axios": "^0.21.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-compose": "^7.22.1",
    "graphql-compose-mongoose": "^9.0.0",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.7.0",
    "mongoose": "^5.10.15",
    "node-fetch": "^2.6.1",
    "shopify-api-node": "^3.5.2",
    "shopify-buy": "^2.11.0",
    "square-connect": "4.20201028.5",
    "twilio": "^3.51.0",
    "uuid": "^8.3.1",
    "xml2js": "^0.4.23"
  }
}
