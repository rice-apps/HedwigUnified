{
  "name": "hedwig-backend",
  "version": "1.0.0",
  "description": "GraphQL backend for Hedwig",
  "repository": {
    "type": "git"
  },
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf build",
    "compile": "babel src -d build",
    "lint": "prettier-standard --lint",
    "pkg": "pkg build/index.js -o build/HedwigBackend --options trace-warnings",
    "refresh": "rm -rf build node_modules && rm package-lock.json",
    "start": "nodemon",
    "server": "node --trace-warnings build/index.js",
    "create-env": "printenv > .env"
  },
  "author": "vms2@rice.edu",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "nodemon": "^2.0.4",
    "pkg": "^4.4.9",
    "prettier-standard": "^16.4.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "apollo-server-express": "^2.17.0",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.0.0",
    "graphql-compose": "^7.20.1",
    "graphql-compose-mongoose": "^7.5.0",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.7.0",
    "mongoose": "^5.10.3",
    "shopify-buy": "^2.11.0",
    "square-connect": "4.20200826.3",
    "uuid": "^8.3.0",
    "xml2js": "^0.4.23"
  }
}
